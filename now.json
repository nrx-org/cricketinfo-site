{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "routes": [
    {
      "src": "/wiki/(?<lang>.*)",
      "dest": "/home?lang=$lang"
    },
    {
      "src": "/wiki/(?<lang>.*)/(?<articleId>.*)",
      "dest": "/article?lang=$lang&articleId=$articleId"
    }
  ],
  "name": "wikipedia-client-prototypes",
  "alias": ["wikipedia.obvs.io"]
}
