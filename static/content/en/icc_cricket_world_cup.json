    {
      "title": "Events",
      "cardType": "vertical_timeline",
      "facts": [
        {
          "label": "Before the World Cup",
          "note": "-1974",
          "id": "before_the_world_cup",
          "value": {
            "label": "The first international cricket match was played between Canada and the United States, on 24 and 25 September 1844. The first credited Test match was played in 1877 between Australia and England, and the two teams competed regularly for The Ashes in subsequent years.",
            "facts": []
          }
        },
        {
          "label": "Prudential World Cups",
          "note": "1975–1983",
          "id": "prudential_world_cups",
          "value": {
            "label": "The inaugural Cricket World Cup was hosted in 1975 by England. West Indies won the tournament. The 1979 World Cup saw the introduction of the ICC Trophy competition to select non-Test playing teams for the World Cup. The 1983 event was hosted by England for a third consecutive time. ",
            "facts": []
          }
        },
        {
          "label": "Different champions",
          "note": "1987–1996",
          "id": "different_champions",
          "value": {
            "label": "India and Pakistan jointly hosted the 1987 tournament, the first time that the competition was held outside England. The 1992 World Cup, held in Australia and New Zealand, introduced many changes to the game, such as coloured clothing, white balls, day/night matches, and a change to the fielding restriction rules.",
            "facts": []
          }
        },
        {
          "label": "Australian treble",
          "note": "1999–2007",
          "id": "australian_treble",
          "value": {
            "label": "Australia won the ICC World Cup three times consecutively in 1999, 2003 and 2007, completing the first World Cup hat-trick.",
            "facts": []
          }
        }
      ]
    },
