      "facts": [
        {
          "label": "प्राचीन और प्रारंभिक मध्ययुगीन काल",
          "id": "history_phase_1_title",
          "note": "6 वीं शताब्दी ई.पू.",
          "value": {
            "label": "कम से कम 6वीं शताब्दी ईसा पूर्व के बाद से दिल्ली में लगातार निवास का प्रमाण है। सबसे पुराने वास्तुशिल्प अवशेष मौर्य काल से मिलते हैं। दिल्ली में आठ प्रमुख शहरों के अवशेष खोजे गए हैं।"
          }
        },
        {
          "label": "मध्ययुगीन काल",
          "id": "history_phase_2_title",
          "note": "11वीं -14वीं शताब्दी ईस्वी",
          "value": {
            "label": "दिल्ली सल्तनत की नींव रखी गई थी। दिल्ली इस अवधि के दौरान सूफीवाद का एक प्रमुख केंद्र था।"
          }
        },
        {
          "label": "मुग़ल काल",
          "id": "history_phase_3_title",
          "note": "15वीं-18वीं सदी ईस्वी",
          "value": {
            "label": "बाबर ने भारत पर आक्रमण किया और दिल्ली और आगरा से शासन करने वाले मुगल साम्राज्य की स्थापना की।"
          }
        },
        {
          "label": "उपनिवेश काल",
          "id": "history_phase_4_title",
          "note": "19वीं शताब्दी",
          "value": {
            "label": "1857 के सिपाही विद्रोह के दौरान, सीज ऑफ दिल्ली के नाम से जानी जाने वाली लड़ाई के बाद दिल्ली ईस्ट इंडिया कंपनी की सेनाओं के अधीन हो गई। यह शहर 1858 में ब्रिटिश सरकार के सीधे नियंत्रण में आ गया।"
          }
        }
